{
  "name": "graphql-things",
  "version": "0.2.7",
  "description": "P2P GraphQL for the Internet of Things",
  "main": "dist/index.js",
  "repository": "https://github.com/d1plo1d/graphql-things",
  "author": "Rob Gilson",
  "license": "MIT",
  "private": false,
  "files": [
    "dist/*",
    "dist/**/*"
  ],
  "scripts": {
    "bootstrap": "yarn && yarn build && yarn build-example && yarn create-example-keys",
    "build-example": "cd ./example && npx webpack --mode=development",
    "create-example-keys": "babel-node ./scripts/createKeys.js",
    "build-docs": "mmdc -i ./docs/connection-comparison.mmd -o ./docs/dist/connection-comparison.svg -t neutral -b transparent",
    "build": "babel src --out-dir dist && yarn build-example",
    "watch": "babel src --out-dir dist --watch",
    "example": "cd ./example/ && DEBUG=\"*,-babel:*\" yarn start",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "^9.0.0-beta.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-immutablejs": "^0.1.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.20.2",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-monorepo": "^0.2.0",
    "eslint-plugin-react": "^7.11.0",
    "fast-async": "^6.3.8",
    "graphql": "^14.0.2",
    "jest": "^23.6.0",
    "mermaid": "^8.0.0",
    "mermaid.cli": "^0.5.1",
    "msgpack-lite": "^0.1.26",
    "subscriptions-transport-ws": "^0.9.15"
  },
  "peerDependencies": {
    "graphql": "^14.0.2",
    "msgpack-lite": "^0.1.26",
